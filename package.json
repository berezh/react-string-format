        "url": "https://github.com/berezh/react-string-format/issues"
    },
    "homepage": "https://github.com/berezh/react-string-format#readme",
    "devDependencies": {
        "@types/jest": "^24.0.19",
        "@types/react": "^16.9.17",
        "@types/react-test-renderer": "^16.9.2",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "react": "^16.12.0",
        "react-test-renderer": "^16.13.0",
        "ts-jest": "^24.1.0",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
